        hat('RockAccessory','Left Arm',CFrame.new(0,0,0),CFrame.new(0+0*math["sin"](sine/10),90+0*math["sin"](sine/10),0+0*math["sin"](sine/10))*CFrame.Angles(math.rad(0+0*math["sin"](sine/10)),math.rad(0+0*math["sin"](sine/10)),math.rad(0+0*math["sin"](sine/10))),true)
        end
        srv.RenderStepped:Wait()
        end
        end)()
        
	end,
})

